{
  "name": "meme-sites",
  "version": "1.0.0",
  "description": "A collection of simple, responsive meme websites",
  "private": true,
  "workspaces": [
    "sites/*"
  ],
  "scripts": {
    "dev:boffum": "cd sites/boffum && npx serve -p 3001",
    "dev:moreyouknow": "cd sites/themoreyouknow && npx serve -p 3002",
    "dev:pikachu": "cd sites/surprised-pikachu && npx serve -p 3003",
    "dev": "npx concurrently \"npm run dev:boffum\" \"npm run dev:moreyouknow\" \"npm run dev:pikachu\"",
    "build": "npm run build --workspaces"
  },
  "keywords": [
    "meme",
    "website",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
