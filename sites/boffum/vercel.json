{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "headers": {
        "cache-control": "s-maxage=1, stale-while-revalidate"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ],
  "domains": [
    {
      "domain": "boffum.vercel.app",
      "customDomain": "boffum.com"
    }
  ]
}
